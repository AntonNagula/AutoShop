@{
    ViewBag.Title = "Contact";
}
<html>
<head>
    <link type="text/css" rel="stylesheet" href="~/Content/Site.css" />
</head>
<body>
    <div id="start">
        <h4>  Выберите файл для загрузки</h4>
        @using (Html.BeginForm("Contact", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input class="enter" type="file" name="upload" required="required" /><br>
            <input class="enter" type="text" name="Name" required="required" placeholder="введите название модели" /><br>
            <input class="enter" type="text" name="CarBrand" required="required" placeholder="введите название бренда" /><br>
            <input class="enter" type="text" pattern="^[ 0-9]+$" name="Count_Of_Unit" required="required" placeholder="введите колличество моделей" /><br>
            <input class="enter" type="text" name="Price" required="required" placeholder="введите цену моделей" /><br>
            <textarea name="Info" ></textarea>
            <input class="enter" type="submit" value="Загрузить" />
        }
    </div>
    </body>
</html>